<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" android:id="@+id/nav_main"
    app:startDestination="@id/explore">
    <fragment
        android:id="@+id/explore"
        android:name="com.mayburger.eatclone.ui.main.explore.ExploreFragment"
        android:label="ExploreFragment" >
        <action
            android:id="@+id/action_explore_to_reservations"
            app:destination="@id/reservations" />
    </fragment>
    <fragment
        android:id="@+id/reservations"
        android:name="com.mayburger.eatclone.ui.main.reservations.ReservationsFragment"
        android:label="ReservationsFragment" >
        <action
            android:id="@+id/action_reservations_to_supportFragment"
            app:destination="@id/support" />
    </fragment>
    <fragment
        android:id="@+id/support"
        android:name="com.mayburger.eatclone.ui.main.support.SupportFragment"
        android:label="SupportFragment" >
        <action
            android:id="@+id/action_supportFragment_to_moreFragment"
            app:destination="@id/more" />
    </fragment>
    <fragment
        android:id="@+id/more"
        android:name="com.mayburger.eatclone.ui.main.more.MoreFragment"
        android:label="MoreFragment" />
</navigation>